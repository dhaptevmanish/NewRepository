<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Login suite">

	<test name="BothCorrect">
		<parameter name="username" value="Admin"/>
		<parameter name="password" value="admin123"/>
		<classes>
			<class name="Parameters.BothCorrect" />
		</classes>
	</test>
	
	<test name="BothWrong">
		<parameter name="username" value="Admin"></parameter>
		<parameter name="password" value="admin123"></parameter>
		<classes>
			<class name="Parameters.BothWrong" />
		</classes>
	</test>

	<test name="CorrectUsername">
		<parameter name="username" value="Admin"></parameter>
		<parameter name="password" value="admin123"></parameter>
		<classes>
			<class name="Parameters.CorrectUsername" />
		</classes>
	</test>
	<test name="CorrectPassword">
		<parameter name="username" value="Admin"></parameter>
		<parameter name="password" value="admin123"></parameter>
		<classes>
			<class name="Parameters.CorrectPassword" />
		</classes>
	</test>
	
</suite>
